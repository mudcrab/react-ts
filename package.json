{
  "name": "react-ts",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "yarn build:css:dev && webpack-dev-server --hot --inline --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "build:css:dev": "postcss src/index.css -o src/scss/_tailwind.scss",
    "build:css": "postcss src/index.css -o src/scss/_tailwind.scss --env production",
    "lint": "tslint -c tslint.json 'src/**/*.ts*'",
    "prebuild": "NODE_ENV=production yarn build:css"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@rematch/core": "^1.3.0",
