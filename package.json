{
  "name": "react-ts-template",
  "version": "0.3.0",
  "main": "index.js",
  "license": "MIT",
  "author": "Jevgeni Sillar <jevgeni@pitfire.io>",
  "scripts": {
    "dev": "yarn build:css:dev && webpack-dev-server --mode=development --hot --inline --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "build:css:dev": "postcss src/index.css -o src/scss/_tailwind.scss",
    "build:css": "postcss src/index.css -o src/scss/_tailwind.scss --env production",
    "stats": "webpack --config webpack.prod.js --profile --json > dist/stats.json",
    "analyze": "webpack-bundle-analyzer dist/stats.json",
    "lint": "tslint -c tslint.json 'src/**/*.ts*'",
    "prebuild": "NODE_ENV=production yarn build:css"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@rematch/core": "^1.3.0",
    "@types/node": "^12.12.14",
    "@types/react": "^16.8.20",
    "@types/react-dom": "^16.9.4",
    "@types/react-router": "^5.1.3",
    "@types/react-router-dom": "^5.1.3",
    "autoprefixer": "^9.7.2",
    "awesome-typescript-loader": "^5.2.1",
    "ky": "^0.16.1",
    "node-sass": "^4.10.0",
    "postcss-cli": "^6.1.3",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-async-hook": "^3.6.1",
    "react-dom": "npm:@hot-loader/react-dom",
    "react-feather": "^2.0.3",
    "react-hook-form": "^3.28.7",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "speed-measure-webpack-plugin": "^1.3.3",
    "style-loader": "^0.23.1",
    "tailwindcss": "^1.1.4",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "tslint-react": "^4.1.0",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "class-validator": "^0.9.1",
    "classnames": "^2.2.6",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.0.0",
    "date-fns": "^2.8.1",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "lodash.difference": "^4.5.0",
    "lodash.get": "^4.4.2",
    "mdi-react": "^6.5.0",
    "preact": "^10.0.5",
    "react-hot-loader": "^4.12.18",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "rxjs": "^6.5.4",
    "terser-webpack-plugin": "^2.2.1",
    "tslint": "^5.20.0",
    "typescript": "^3.7.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^4.2.2"
  },
  "resolutions": {
    "**/**/set-value": "^2.0.1"
  }
}
