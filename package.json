    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "terser-webpack-plugin": "^2.2.1",
    "tslint": "^5.20.0",
    "typescript": "^3.7.2",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^4.2.2"
  }
