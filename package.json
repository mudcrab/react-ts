    "autoprefixer": "^9.7.2",
    "awesome-typescript-loader": "^5.2.1",
    "ky": "^0.16.1",
    "node-sass": "^4.10.0",
    "postcss-cli": "^6.1.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hook-form": "^3.28.7",
    "react-redux": "^7.1.3",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "tailwindcss": "^1.1.4",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
